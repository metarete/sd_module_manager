{{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}


<div class="row">
	<div class="col">
		{{ form_row(form.name) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.surname) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.email) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.username) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.cf) }}
	</div>
</div>
<div class="row">
	<label for="inputPassword" class="form-label">Password nuova</label>
	<div class="col">
		<input type="password" class="form-control" id="inputPassword2" name='password_nuova'>
	</div>
</div>
<br>
<div class="row">
	<label for="inputPassword" class="form-label">Conferma Password</label>
	<div class="col">
		<input type="password" class="form-control" id="inputPassword3" name='conferma_password'>
	</div>
</div>
<br>
<button class="btn btn-primary">{{ button_label|default('Salva') }}</button>
{{ form_end(form) }}

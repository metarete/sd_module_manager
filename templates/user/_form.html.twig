{{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}


<div class="row">
	<div class="col">
		{{ form_row(form.name) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.surname) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.email) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.cf) }}
	</div>
</div>
<div class="row">
	<div class="col">
		{{ form_row(form.password) }}
	</div>
</div>
<br>
<button class="btn btn-primary">{{ button_label|default('Salva') }}</button>
{{ form_end(form) }}

											<th>{{scheda_pai.dataInizio ? scheda_pai.dataInizio|date('d/m/Y') : ''}} -- {{scheda_pai.dataFine ? scheda_pai.dataFine|date('d/m/Y') : ''}}</th>
										{% endfor %}
									</th>
								</tr>
